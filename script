cd C:\Program Files\Tableau\Tableau Server\2020.3\extras\Command Line Utility
tabcmd.exe
tabcmd login --no-proxy -s http://tableau-2020-3-production.datateam.jimdo-server.com -u jimdoadmin -p %TABCMD_JIMDO_ADMIN_PASS%

::Get New Sales Distribution
tabcmd export "NewSalesDistributionDashboard/NewSalesDistribution3?:iid=4" --png -f "G:\My Drive\Board reporting\Current_Screenshots\0-sales-dist.png"

